{
  "extends": "airbnb-base",
  "rules": {
      "linebreak-style": off,
      "semi": [2, "never"],
      "no-undef": off,
      "dot-notation": off,
      "arrow-body-style": off,
      "no-unused-vars": off,
      "no-mixed-operators": off,
      "prefer-template": off,
      "no-nested-ternary": off,
      "func-names": off,
      "no-param-reassign": off,
      "object-shorthand": off,
      "no-mutable-exports": off,
    }
}
